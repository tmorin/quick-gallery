{
  "name": "quick-gallery",
  "version": "0.0.0-beta2",
  "description": "Quick gallery",
  "main": "lib/server",
  "keywords": [
    "gallery"
  ],
  "homepage": "https://github.com/tmorin/quick-gallery",
  "repository": {
    "type": "git",
    "url": "https://github.com/tmorin/quick-gallery.git"
  },
  "bugs": {
    "url": "https://github.com/tmorin/quick-gallery/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/tmorin/quick-gallery/blob/master/LICENSE"
    }
  ],
  "author": "Thibault Morin (https://tmorin.github.io/)",
  "scripts": {
    "clean": "rimraf bin lib public *.tgz",
    "build:lib": "babel src/lib --out-dir lib",
    "build:bin": "babel src/bin --out-dir bin",
    "build:public": "webpack --config webpack.config.production.js",
    "watch:srv": "nodemon --watch src/lib --exec babel-node src/lib/server.js",
    "watch:site": "webpack-dev-server --config webpack.config.development.js --inline --hot",
    "build": "npm run build:bin && npm run build:lib && npm run build:public",
    "postversion": "git push && git push --tags",
    "prepublish": "npm run build",
    "start:srv": "node lib/server.js",
    "start": "node lib/start.js"
  },
  "bin": {
    "qg-cache-builder": "bin/qg-cache-builder.js",
    "qg-server": "bin/qg-server.js"
  },
  "config": {
    "http_port": 4000,
    "adapted_max_width": 1000,
    "adapted_max_height": 1000,
    "thumbnail_max_width": 120,
    "thumbnail_max_height": 120,
    "imagemagick_path": "/usr/bin"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "backbone": "^1.2.1",
    "backbone.localstorage": "^1.1.16",
    "backbone.marionette": "^3.1.0",
    "backbone.stickit": "^0.9.2",
    "bootstrap": "^3.3.4",
    "css-loader": "^0.26.1",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.3.0",
    "form-serializer": "^2.0.1",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
    "jquery": "^3.1.1",
    "jquery-lazyload": "^1.9.5",
    "jshint-stylish": "^2.0.0",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "less-plugin-autoprefix": "^1.4.2",
    "less-plugin-clean-css": "^1.5.0",
    "lodash": "^4.17.2",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "async": "^2.1.4",
    "body-parser": "^1.12.4",
    "express": "^4.12.4",
    "glob": "^7.1.1",
    "gm": "^1.18.1",
    "jszip": "^3.1.3",
    "minimist": "^1.1.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.4",
    "walk": "^2.3.9",
    "winston": "^2.3.0"
  }
}
